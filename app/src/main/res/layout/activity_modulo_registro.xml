<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modulo_registro">

    <ImageView
        android:id="@+id/id_m3_img1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/wave_1" />

    <Button
        android:id="@+id/id_m3_btn2"
        android:layout_width="37dp"
        android:layout_height="47dp"
        android:background="@drawable/ic_btn2_regresar"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/id_m3_tit1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m3_tit1"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.212"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112" />

    <TextView
        android:id="@+id/id_m3_sub1"
        android:layout_width="297dp"
        android:layout_height="95dp"
        android:text="@string/m3_sub1"
        android:textColor="#FFFFFF"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.187" />

    <EditText
        android:id="@+id/id_m3_edittxt1"
        android:layout_width="300dp"
        android:layout_height="56dp"
        android:background="@drawable/an_cjt_1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_txt1"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/id_m3_txt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m3_txt1"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.339" />

    <TextView
        android:id="@+id/id_m3_txt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m3_txt2"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_edittxt1"
        app:layout_constraintVertical_bias="0.034" />

    <EditText
        android:id="@+id/id_m3_edittxt2"
        android:layout_width="300dp"
        android:layout_height="56dp"
        android:background="@drawable/an_cjt_1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Correo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_txt2"
        app:layout_constraintVertical_bias="0.038" />

    <TextView
        android:id="@+id/id_m3_txt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m3_txt3"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_edittxt2"
        app:layout_constraintVertical_bias="0.05" />

    <EditText
        android:id="@+id/id_m3_edittxt3"
        android:layout_width="300dp"
        android:layout_height="56dp"
        android:background="@drawable/an_cjt_1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="********"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_txt3"
        app:layout_constraintVertical_bias="0.067" />

    <Button
        android:id="@+id/id_m3_btn1"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/an_btn_1"
        android:elevation="15dp"
        android:text="@string/m3_btn1"
        android:textColor="#FFFAFA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_checkBox1"
        app:layout_constraintVertical_bias="0.811" />

    <CheckBox
        android:id="@+id/id_m3_checkBox1"
        android:layout_width="318dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:text="@string/m3_txt4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_m3_edittxt3"
        app:layout_constraintVertical_bias="0.056" />

</androidx.constraintlayout.widget.ConstraintLayout>