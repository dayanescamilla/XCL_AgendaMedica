<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Perfil">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <View
            android:id="@+id/view3"
            android:layout_width="0dp"
            android:layout_height="200dp"
            android:background="@drawable/wave_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ScrollView
            android:id="@+id/scrollView5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/id_m12_btn2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view3">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="24dp"
                android:layout_marginRight="24dp"
                android:orientation="vertical">


                <TextView
                    android:id="@+id/id_m12_txt1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt1"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />

                <TextView
                    android:id="@+id/id_m12_cjt1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt1"
                    android:textSize="@dimen/dimDescripicon" />

                <TextView
                    android:id="@+id/id_m12_txt2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt2"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />

                <TextView
                    android:id="@+id/id_m12_cjt2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt2"
                    android:textSize="@dimen/dimDescripicon" />


                <TextView
                    android:id="@+id/id_m12_txt3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt3"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />

                <TextView
                    android:id="@+id/id_m12_cjt3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt3"
                    android:textSize="@dimen/dimDescripicon" />


                <TextView
                    android:id="@+id/id_m12_txt4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt4"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />

                <TextView
                    android:id="@+id/id_m12_cjt4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt4"
                    android:textSize="@dimen/dimDescripicon" />

                <TextView
                    android:id="@+id/id_m12_txt5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt5"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />

                <TextView
                    android:id="@+id/id_m12_cjt5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt5"
                    android:textSize="@dimen/dimDescripicon" />

                <TextView
                    android:id="@+id/id_m12_txt6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_txt6"
                    android:textColor="@color/TextosNegro"
                    android:textSize="@dimen/dimSubtitulos" />


                <TextView
                    android:id="@+id/id_m12_cjt6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/m12_cjt6"
                    android:textSize="@dimen/dimDescripicon" />
            </LinearLayout>
        </ScrollView>


        <TextView
            android:id="@+id/id_m12_tit1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="24dp"
            android:text="@string/m12_tit1"
            android:textColor="#FFFFFF"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.07999998" />

        <Button
            android:id="@+id/id_m12_btn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/an_btn_2"
            android:text="@string/m12_btn1"
            android:textColor="@color/TextosBlanco"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintTop_toBottomOf="@+id/id_m12_tit1" />

        <Button
            android:id="@+id/id_m12_btn2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:text="@string/m12_btn2"
            android:layout_marginBottom="24dp"
            android:background="@drawable/an_btn_3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>